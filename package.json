{
  "name": "material-components",
  "version": "0.0.3",
  "description": "Stateless UI components for react that follow material design",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src test",
    "dev": "parallelshell 'babel src --watch --out-dir lib' 'watch --wait=1 \"eslint src test && mocha --compilers js:babel/register --recursive --reporter dot\" src test' 'node-sass --watch src/index.scss lib/index.css'",
    "prebuild": "npm run lint && rimraf lib",
    "build": "babel src --out-dir lib && node-sass src/index.scss lib/index.css",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garth/material-components.git"
  },
  "keywords": [
    "react",
    "material",
    "component"
  ],
  "author": "Garth Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garth/material-components/issues"
  },
  "homepage": "https://github.com/garth/material-components#readme",
  "dependencies": {
    "node-waves": "^0.7.2"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "chai": "^3.2.0",
    "eslint": "^1.3.1",
    "mocha": "^2.3.2",
    "node-sass": "^3.3.3",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.4.3",
    "watch": "^0.16.0"
  },
  "peerDependencies": {
    "classnames": "^2.1.5",
    "react": "0.14.0-rc1",
    "react-addons-css-transition-group": "0.14.0-rc1",
    "react-dom": "0.14.0-rc1",
    "react-tap-event-plugin": "0.2.0"
  }
}
