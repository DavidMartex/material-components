{
  "name": "material-components",
  "version": "0.0.40",
  "description": "Stateless UI components for react that follow material design",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src test",
    "dev": "parallelshell 'babel src --watch --out-dir lib' 'watch --wait=1 \"eslint src test && mocha --compilers js:babel/register --recursive --reporter dot\" src test' 'node-sass --watch src/index.scss lib/index.css' 'webpack --watch --config demo/webpack.config.js'",
    "prebuild": "npm run lint && rimraf lib",
    "build": "babel src --out-dir lib && node-sass src/index.scss lib/index.css && webpack --config demo/webpack.config.js",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garth/material-components.git"
  },
  "keywords": [
    "appbar",
    "button",
    "calendar",
    "checkbox",
    "component",
    "dialog",
    "form",
    "grid",
    "input",
    "material design",
    "material",
    "menu",
    "react",
    "react-component",
    "select",
    "sidenav",
    "table"
  ],
  "author": "Garth Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garth/material-components/issues"
  },
  "homepage": "https://github.com/garth/material-components#readme",
  "dependencies": {
    "node-waves": "^0.7.4"
  },
  "devDependencies": {
    "babel": "^6.0.2",
    "babel-core": "^6.0.2",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.3",
    "bootstrap-sass": "^3.3.5",
    "chai": "^3.4.0",
    "css-loader": "^0.21.0",
    "eslint": "^1.7.3",
    "eslint-plugin-react": "^3.6.3",
    "mocha": "^2.3.2",
    "node-sass": "^3.4.1",
    "normalize.css": "^3.0.3",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "watch": "^0.16.0",
    "webpack": "^1.12.2"
  },
  "peerDependencies": {
    "classnames": "^2.2.0",
    "moment": "^2.10.6",
    "react": "^0.14.0",
    "react-addons-css-transition-group": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-tap-event-plugin": "^0.2.1"
  }
}
